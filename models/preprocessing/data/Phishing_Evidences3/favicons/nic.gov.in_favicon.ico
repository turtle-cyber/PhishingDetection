                    if (!slider.playing) {
                        if (lastNav === "next") {
                            slider.find(".flex-direction-nav .flex-next").focus();
                        } else if (lastNav === "prev") {
                            slider.find(".flex-direction-nav .flex-prev").focus();
                        }
                        lastNav = null; // reset
                    }

                    let start = (slider.currentSlide * slider.visible ) + 1;

                    let end = Math.min((start + slider.visible) - 1,slider.count)
                   
